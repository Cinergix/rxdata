{
  "name": "rxdata",
  "version": "3.2.1",
  "description": "A reactive document database for web browsers",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage --watch",
    "lint": "prettier --print-width 120 --single-quote --trailing-comma es5 --write \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cinergix/rxdata.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cinergix/rxdata/issues"
  },
  "homepage": "https://github.com/cinergix/rxdata#readme",
  "peerDependencies": {
    "rxjs": "^5.0.0"
  },
  "dependencies": {
    "localforage": "^1.4.3",
    "mingo": "^1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.0",
    "jest": "^20.0.0",
    "prettier": "^1.5.3",
    "rxjs": "^5.0.0",
    "ts-jest": "^20.0.0",
    "typescript": "^2.0.0"
  },
  "jest": {
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/index.ts",
      "!src/__test_utils.ts",
      "!src/**/*.mock.ts",
      "!src/**/*.d.ts"
    ]
  }
}
